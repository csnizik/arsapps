{
  "name": "drupal-project",
  "version": "1.0.0",
  "description": "Drupal 11 project with frontend build tools",
  "private": true,
  "scripts": {
    "build": "cd web/themes/custom/arsapps_theme && npm run build",
    "dev": "cd web/themes/custom/arsapps_theme && npm run dev",
    "lint": "cd web/themes/custom/arsapps_theme && npm run lint",
    "test": "npm run test:unit && npm run test:accessibility && npm run test:e2e",
    "test:unit": "vendor/bin/phpunit --testsuite=custom",
    "test:accessibility": "npm run test:axe && npm run test:pa11y",
    "test:axe": "axe http://localhost:8080 --tags section508,wcag2a,wcag2aa",
    "test:pa11y": "pa11y http://localhost:8080",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:lighthouse": "lighthouse http://localhost:8080 --output=html --output-path=reports/lighthouse.html",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.2",
